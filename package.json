{
  "name": "lahebo-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build:demo": "vue-tsc --noEmit && vite build --mode demo",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "preview": "vite preview",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore src",
    "lint:style": "stylelint src/**/*.{css,scss,vue}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@headlessui/vue": "^1.6.4",
    "@stripe/stripe-js": "^1.34.0",
    "@types/aws-iot-device-sdk": "^2.2.4",
    "@types/vue-select": "^3.16.0",
    "@vueform/slider": "^2.1.2",
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "@vuepic/vue-datepicker": "^3.2.2",
    "aws-iot-device-sdk": "^2.2.12",
    "aws-iot-device-sdk-v2": "^1.8.5",
    "aws-sdk": "^2.1179.0",
    "axios": "^0.27.2",
    "d3": "^7.6.1",
    "floating-vue": "^2.0.0-beta.17",
    "libphonenumber-js": "^1.10.7",
    "maska": "^1.5.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.14",
    "qrcode": "^1.5.0",
    "v-tooltip": "^2.1.3",
    "vue": "^3.2.25",
    "vue-color-kit": "^1.0.5",
    "vue-loading-overlay": "^5.0.3",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-router": "^4.0.15",
    "vue-select": "^4.0.0-beta.3",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-table-lite": "^1.2.3",
    "vue3-tel-input": "^1.0.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/d3": "^7.4.0",
    "@types/node": "^17.0.45",
    "@types/qrcode": "^1.4.2",
    "@types/vue-select": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
